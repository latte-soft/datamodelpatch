    ["Authentication.TwoStepVerification.Description.TwoStepVerificationAuthenticatorCustomerSupportDeactivationEmail.Subject"] = "Verifikasi Dua Langkah dengan Authenticator Dinonaktifkan oleh Dukungan Pelanggan Roblox: {accountName}", 
